{
  "name": "@baker-fi/sdk",
  "version": "1.4.0-rc.1",
  "description": "",
  "main": "dist/src/types/index.js",
  "module": "dist/src/types/index.js",
  "files": [
    "files",
    "artifacts/contracts",
    "dist",
    "src/contract-blob.json",
    "src/contract-selectors.json",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf cache/* && rm -rf artifacts/*  && rm -rf dist/*",
    "compile": "npx hardhat compile",
    "postcompile": "npx hardhat build:artifactTree && npx hardhat build:calculateSelectors",
    "build": "npm run compile && npx tsc && npx tsc-alias",
    "test": "npx hardhat test",
    "solhint:check": "npx solhint  'contracts/**/*.sol'",
    "prebuild-manual": "sh scripts/build-models.sh",
    "doc": "npx hardhat docgen",
    "test:gas": "REPORT_GAS=true npx hardhat test",
    "test:coverage": "REPORT_GAS=true npx hardhat coverage",
    "contract:size": "npx hardhat size-contracts",
    "lint": "npx hardhat check",
    "prettier": "npx prettier --plugin=prettier-plugin-solidity --config .prettierrc.json --write '**/*.{js,ts,json,sol}'",
    "start": "NODE_ENV=production node dist/index.js",
    "export:flat": "npx hardhat export-flat",
    "export:selectors": "npx hardhat selectors",
    "deploy:hardhat": "npx hardhat run --network hardhat scripts/deploy.ts",
    "deploy:local": "npx hardhat run --network local scripts/deploy-dev.ts",
    "deploy:ethereum": "npx hardhat run --network ethereum scripts/deploy.ts",
    "deploy:ethereum-devnet": "npx hardhat run --network ethereum_devnet scripts/deploy.ts",
    "deploy:arbitrum": "npx hardhat run --network arbitrum scripts/deploy.ts",
    "deploy:arbitrum-devnet": "npx hardhat run --network arbitrum_devnet scripts/deploy.ts",
    "deploy:optimism": "npx hardhat run --network optimism scripts/deploy.ts",
    "deploy:optimism-devnet": "npx hardhat run --network optimism_devnet scripts/deploy.ts",
    "deploy:base": "npx hardhat run --network base scripts/deploy.ts",
    "deploy:base-devnet": "npx hardhat run --network base_devnet scripts/deploy.ts",
    "ganache:dev": "ganache -h 0.0.0.0  --logging.debug --logging.verbose -e 1000000 -p 8545 -a 50  -s 0x6d2119524d65ea0b15b214ef260d3acdf8ca0ec6ec68045b5785d970f5ad97bd3d0025d212e625a8a21920852cc165c10faa6d8387a0c2530d1ffc0d265d92935749",
    "slither": "slither --config-file ./slither.config.json ."
  },
  "author": "Chef Kenji <chef.kenji@bakerfi.xyz>",
  "license": "ISC",
  "devDependencies": {
    "@aave/core-v3": "^1.18.0",
    "@ledgerhq/hw-app-eth": "^6.38.0",
    "@ledgerhq/hw-transport-node-hid": "^6.29.3",
    "@morpho-org/morpho-blue": "1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.10",
    "@nomiclabs/hardhat-solhint": "^4.0.1",
    "@openzeppelin/contracts": "^4.9.2",
    "@openzeppelin/contracts-upgradeable": "^4.9.2",
    "@pythnetwork/price-service-client": "^1.9.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.19",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.3.2",
    "@types/ora": "^3.2.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.13.2",
    "ganache": "^7.9.0",
    "hardhat": "^2.22.10",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-docgen": "^1.3.0",
    "hardhat-flat-exporter": "^1.0.1",
    "hardhat-storage-layout-changes": "^0.1.2",
    "hardhat-tracer": "^3.1.0",
    "ora": "^3.4.0",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.4.1",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-docgen": "^0.6.0-beta.36",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.2"
  },
  "dependencies": {
    "ethers-decode-error": "^2.1.2"
  },
  "contractSizer": {
    "alphaSort": true,
    "disambiguatePaths": false,
    "runOnCompile": true,
    "strict": true,
    "only": [
      ":ERC20$"
    ]
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  }
}
