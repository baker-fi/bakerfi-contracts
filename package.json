{
  "name": "landromat",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "files": [
    "files",
    "dist",
    "README.md"
  ],
  "scripts": {
    "compile:clean": "rm -rf cache/* && rm -rf artifacts/* && rm -rf typechain-types/*",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "test:gas": "REPORT_GAS=true npx hardhat test",
    "test:coverage": "REPORT_GAS=true npx hardhat coverage",
    "contract:size": "npx hardhat size-contracts",
    "lint": "npx hardhat check",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "start": "NODE_ENV=production node dist/index.js",
    "deploy:hardhat": "npx hardhat run --network hardhat scripts/deploy.ts",
    "deploy:local": "npx hardhat run --network local scripts/deploy.ts",
    "deploy:polygon": "npx hardhat run --network polygon scripts/deploy.ts",
    "deploy:mumbai": "npx hardhat run --network mumbai scripts/deploy.ts",
    "ganache:dev": "ganache -h 0.0.0.0  --logging.debug --logging.verbose -e 100000 -p 8545 -a 50  -s 0x6d2119524d65ea0b15b214ef260d3acdf8ca0ec6ec68045b5785d970f5ad97bd3d0025d212e625a8a21920852cc165c10faa6d8387a0c2530d1ffc0d265d92935749"
  },
  "author": "Helder Vasconcelos <helder@layerx.xyz>",
  "license": "ISC",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-solhint": "^3.0.1",
    "@taikai/dappkit-launchpad": "^0.0.19",
    "@typechain/hardhat": "^8.0.0",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.2",
    "ethers": "^6.7.1",
    "ganache": "^7.9.0",
    "hardhat": "^2.16.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.3",
    "ts-node": "^10.9.1",
    "typechain": "^8.2.0"
  },
  "dependencies": {
    "@aave/core-v3": "^1.18.0",
    "@openzeppelin/contracts": "^4.9.2",
    "@sushiswap/bentobox-sdk": "^1.0.0-canary.13",
    "@taikai/dappkit": "^2.3.1"
  },
  "contractSizer": {
    "alphaSort": true,
    "disambiguatePaths": false,
    "runOnCompile": true,
    "strict": true,
    "only": [
      ":ERC20$"
    ]
  }
}
